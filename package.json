{
  "name": "Loginized",
  "description": "Loginized Gnome GDM Login Theme Manager",
  "version": "0.1.4-SNAPSHOT",
  "private": true,
  "main": "./electron/main.js",
  "license": "GPL-3.0",
  "author": {
    "name": "Juha Kukkonen",
    "email": "juha7kukkonen@gmail.com",
    "url": "https://github.com/juhaku/loginized"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test",
    "lint": "vue-cli-service lint",
    "erun": "RUNTIME=ALL NODE_ENV=development electron-forge start"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "zip",
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "./src/assets/icon_3@3x.png"
      },
      "electronRebuildConfig": {},
      "electronWinstallerConfig": {},
      "electronInstallerDMG": {},
      "electronInstallerFlatpak": {},
      "electronInstallerDebian": {
        "icon": "./src/assets/icon_3@3x.png",
        "categories": [
          "Utility"
        ]
      },
      "electronInstallerRedhat": {
        "icon": "./src/assets/icon_3@3x.png",
        "categories": [
          "Utility"
        ]
      },
      "electronInstallerSnap": {}
    }
  },
  "dependencies": {
    "@types/lodash": "^4.14.104",
    "@types/opn": "^5.1.0",
    "electron-compile": "^6.4.2",
    "lodash": "^4.17.5",
    "luxon": "^1.0.0",
    "opn": "^5.3.0",
    "perfect-scrollbar": "^1.3.0",
    "promise": "^8.0.1",
    "vue": "^2.5.13",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^6.0.0",
    "vueify": "^9.4.1",
    "vuetify": "^1.0.4",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.1",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.1",
    "@vue/cli-service": "^3.0.0-beta.1",
    "@vue/devtools": "^1.0.2",
    "@vue/test-utils": "^1.0.0-beta.10",
    "electron-forge": "^5.1.1",
    "electron-prebuilt-compile": "1.8.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^22.0.1",
    "vue-template-compiler": "^2.5.13"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "mapCoverage": true
  }
}
